// Header mobile -----------------------------------------------------------------------------------------------------------
.header-mobile {
	@include box-shadow(0 5px 15px rgba(0, 0, 0, 0.1));
	position: relative;
	background: white;
	padding: 10px 15px;
	.item-mobile-top {
		position: relative;
		cursor: pointer;
		font-size: 20px;
		float: inherit;
		text-shadow: none;
		line-height: 10px;
		i {
			color: #222;
			font-size: 24px;
		}
	}
	#_mobile_menutop {
		@include square(30px);
		text-align: center;
		line-height: 30px;
		background-color: darken(#343434, 5%);
		border-radius: 50%;
		cursor: pointer;
		i {
			font-size: 21px;
		}
	}
	.mobile_logo {
    	@include rtl-margin-right(15px);
	}
	#mobile_search {
		position: relative;
		width: 100%;
    	@include rtl-margin-right(15px);
		input {
			background: #f4f4f4;
		    border: none;
		    @include border-radius(30px);
		    padding: 0 20px;
		    height: 40px;
		    @include placeholder {
		    	font-size: 11px;
		    	font-weight: 500;
		    	color: #b2b2b2;
		    }
		}
		button {
			position: absolute;
			top: 10px;
			@include rtl-right(10px);
			background-color: #f4f4f4;
			.icon__fallback-text {display: none !important;}
			.site-header__search-icon {
				.zmdi-search {
					font-size: 21px;
					color: #000;
				}
			}
		}
	}
	@media (max-width: 575px) {
		.mobile_logo {
			img {
				max-width: 90px !important;
			}
		}
	}
}

.mobile-boxpage {
	position: fixed;
	top: 100%;
	left: 0;
	right: 0;
	bottom: 0;
	background: #fff;
	z-index: 1000;
	visibility: hidden;
	@include opacity(0);
	@include transition(all 0.2s);
	&.active {
		visibility: visible;
		@include opacity(1);
		top: 0;
	}
}
.content-boxpage {
	padding: 0;
	overflow-y: scroll;
	.box-header {
		height: 45px;
		background: rgba(#fff,0.95);
		@include box-shadow(0 2px 5px -2px rgba(#000,0.2));
		padding: 0 15px;
		position: relative;
		z-index: 1001;
		.title-box {
			font-family: $font-family-primary;
			font-weight: 700;
			text-transform: uppercase;
			color: #000;
			font-size: 16px;
		}
		.close-box, .back-box{
			font-size: 11px;
			color: $theme-color-default;
			font-weight: 700;
			cursor: pointer;
		}
		.back-box {
			display: none;
			&.active {
				display: block;
			}
		}
	}
	.box-content {
		overflow-y: scroll;
	}
}

#mobile-pageaccount {
	.account-list-content {
		font-size: 13px;
		i {
			display: none;
		}
		.currency_header {
			@include display(none);
		}
		> div {
			padding: 15px 0;
		}
	}
	.links-currency,.links-language {
		font-size: 13px;
		padding: 15px 0;
		position: relative;
		cursor: pointer;
		i {
			position: absolute;
			@include rtl-right(-20px);
			top: 20px;
		}
	}
	#box-currency, #box-language {
		position: absolute;
		top: 45px;
		left: 0;
		right: 0;
		bottom: 0;
		visibility: hidden;
		@include opacity(0);
		@include translateX(-100%);
		@include transition(all 0.8s ease);
		background: #fff;
		&.active {
			visibility: visible;
			@include opacity(1);
			@include translateX(0);
		}
		.item-language, .item-currency {
			padding: 10px 15px;
			border-bottom: 1px dotted #eaeaea;
			position: relative;
			&.current {
				&:before {
					position: absolute;
					top: 12px;
					@include rtl-right(15px);
					content: '\f26b';
					font: normal normal normal 14px/1 'Material-Design-Iconic-Font';
					font-size: 14px;
					text-rendering: auto;
					color: $theme-color-default;
				}
			}
		}
	}
}

