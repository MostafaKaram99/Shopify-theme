/* Header Sticky -----------------------------------------------------------------------------------*/
#header-sticky {
  @media (min-width: 768px) {
    position: fixed;
    width: 100%;
    top: -48px;
    @include rtl-left(0);
    z-index: 999;
    @include opacity(0);
    visibility: hidden;
    @include box-shadow(0 6px 5px rgba($color-default,0.1));
    @include transition(top 0.45s ease);
    padding: 10.5px 40px;
    &.sticky-menu-active {
      top: 0;
      @include opacity(1);
      visibility: visible;
    }
    &.open {
      position: fixed;
      width: 100%;
      top: 0;
      @include rtl-left(0);
      z-index: 99999;
    }
    .site-header__cart .site-header__cart-count {background-color: $color-black;}
    .label-header {
