          .ajaxcart__inner {
            max-height: 100%;
            overflow: auto;
            overflow-x: hidden;
            .ajaxcart__product {
              position: relative;
              padding: 20px;
              &:not(:last-child) {
                border-bottom: 1px solid #e6e6e6;
              }
              &:first-child {
                padding: 0 20px 20px;
              }
              &:last-child {
                padding: 20px 20px 0;
              }
              .media {
                > a {
                  img {
