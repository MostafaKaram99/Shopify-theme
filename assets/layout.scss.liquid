    .mm-navbar {
      .mm-title {
        color: $color-menu !important;
        font-size: 11px;
        text-transform: uppercase;
        font-weight: 900;
      }
      .mm-prev:before {
        border-color: $color-heading;
        top: -2px;
      }
    }
    .mm-panel {
      visibility: visible;
      @include opacity(1);
      background-image: none !important;
      .mm-listview {
        margin-top: -15px;
        &>li {
          padding: 0;
          margin-bottom: 6px;
          &:not(.mm-divider):after {
            border-bottom: none;
          }
          .mm-next:before {
            @include display(none);
          }
          .mm-next:after {
            border: none;
            content: "\f2f6";
            font-family: "Material-Design-Iconic-Font";
            font-size: 24px;
            color: #b3b4b3;
            @include rotate(0);
            border: none;
            display: contents;
            width: initial;
            height: initial;
          }
        }
        li {
          a {
            font-size: 13px;
            @include rtl-padding-left(20px);
            border-bottom: none;
            padding-top: 5px;
            padding-bottom: 5px;
            font-weight: 600;
            @include flexbox();
            @include align-items(center);
          }
          .site-nav__link--main {
            font-weight: 700;
          }
        }
      }
      &.mm-hasnavbar .mm-navbar {
        max-width: 250px;
        @include rtl-text-align-left();
        border-bottom: none;
      }
    }
    .site-nav__dropdown {
      .menuGroup {
        .novMenuLinks {
          padding: 0;
        }
      }
    }
    .show_sub {display: none;}
  }
  .canvas-header-box {
    padding: 35px 0;
    font-size: 16px;
    font-weight: 700;
    color: #c3c4c3;
    background-color: white;
    .close-box {
      cursor: pointer;
    }
  }
  .site-nav {
    > li {
      @include rtl-text-align-left();
      @include display(block);
      &:before {
        @include display(none);
      }
      >.mm-next {
        padding: 20px 32px;
        &:before {
          @include display(none);
        }
        &:after {
          content: "\f2f6";
          font-family: 'Material-Design-Iconic-Font';
          font-size: 24px;
          color: #b3b4b3;
          @include rotate(0);
          border: none;
          display: contents;
          width: initial;
          height: initial;
        }
      }
      &:not(.mm-divider)::after {
        @include rtl-left(0 !important);
        border-bottom: none;
      }
      >a:not(.mm-next) {
        color: $color-heading;
        font-size: 1.6rem;
        font-weight: 700;
        padding: 15px 0 !important;
        @include rtl-padding-lr(20px !important,10px !important);
      }
      &.site-nav--active {
        a {
          color: $theme-color-primary;
        }
        .mm-next:after {
          color: $theme-color-primary;
        }
      }
      > .site-nav__link {        
        > i {@include display(none);}
      }
    }
  }
  .site-nav__dropdown {
    ul {
      @include rtl-padding-left(20px);
      >li {
        list-style-type: none;
        margin-bottom: 17px;
        >a {
          color: $color-menu;
          font-weight: 500;
          line-height: 22px;
          font-size: 13px;
        }
      }
    }
    .site-nav__link {
      padding-top: 10px;
      padding-bottom: 10px;
    }
    &.megaMenu {
      width: 100% !important;
      .novMenuLinks {
        li {
          margin-bottom: 10px;
          &.menuTitle {
            padding: 10px 0;
            a {
              font-weight: 900;
              text-transform: uppercase;
              font-size: 11px;
            }
          }
          &:not(.menuTitle) {
            @include rtl-margin-left(15px);
            a {
              padding: 5px 0;
              @include display(inline-block);
              color: $color-heading-sub;
              font-size: 12px;
            }
          }
        }
      }
      .d-banner, .d-product {
        width: 100% !important;
        margin-top: 19px;
        padding: 0 !important;
      }
      .list_collection {
        margin-bottom: 20px;
        .cltion {
          &:not(:last-child) {margin-bottom: 15px;}
        }
        a {
          display: block;
          font-weight: 900;
          line-height: 22px;
          text-transform: uppercase;
          display: inline-block;
          font-size: 11px !important;
        }
        span {
          @include rtl-margin-right(15px);
          &:hover {
            text-decoration-line: underline;
            text-decoration-color: #000;
          }
        }
        i {
          font-size: 14px;
        }
      }
      .menu_pr {
        margin-bottom: 10px;
        .title-product {
          font-weight: 900;
          line-height: 22px;
          margin-bottom: 24px;
        }
        .novmenu_product {
          &.slick-slider {
            position: relative;
            .slick-dots {
              position: absolute;
              @include rtl-left(50%);
              @include translateX(-50%);
              bottom: -50px;
              @include rtl-padding-left(0);
              li {
                @include square(10px);
                button {
                  @include square(10px);
                  &:before {
                    @include square(10px);
                    background: #d3d3d3;
                  }
                }
                &.slick-active {
                  button {
                    &:before {
                      background: #000;
                    }
                  }
                }
              }
            }
          }
          .item {padding: 0;}
          .item-product {
            .product__info {
              text-align: center;
              .product__title {margin: 17px 0 10px;}
            }
          }
        }
      }
    }
  }
  .novMenuBanner {
    margin-bottom: 20px;
  }
  .dropdown_menu {padding: 40px 20px;}
}
body {
  font-size: $font-size-base;
  font-family: $font-family-default;
  color: $color-body-text;
  line-height: 1.5;
  font-weight: 500;
  position: relative;
  @include transition(none);
  @include rtl-left(0);
  @include rtl-right(0);
  @media (max-width: 767px) {
    @include transition($transition-drawer);
  }
  &.open {
    overflow: hidden;
  }
  &.show-boxpage {
    overflow: hidden;
  }
  &.loading {
    position: relative;
    .nov__load {
      @include opacity(1);
      visibility:initial;
      @include transition(all 2s ease);
    }
    .canvas-overlay {
      @include opacity(1);
      visibility: visible;
    }
  }
  &.cart_popup_opened.loading {
    .canvas-overlay {
      @include opacity(0);
      visibility: hidden;
    }
  }
  &.canvasmenu-right {
    overflow: hidden;
    @include rtl-right(250px);
    @include rtl-left(-250px);
    .canvas-overlay {
      @include opacity(1);
      visibility: visible;
    }
    .canvas-menu {
      &.drawer-left {
        @include rtl-right(0);
      }
    }
  }
  &.bg-full-active{
    .canvas-overlay{
      @include opacity(1);
      visibility: visible;
    }
  }
  .canvas-overlay, .sidebar-overlay {
    position: absolute;
    @include rtl-right(0);
    @include rtl-left(0);
    top: 0;
    bottom: 0;
    z-index: 99;
    background: rgba(0, 0, 0, 0.5);
    @include opacity(0);
    visibility: hidden;
    @include transition(all 0.3s linear);
    @media (max-width: 767px) {
      @include rtl-left(0);
    }
    &.act {
      @include opacity(1);
      visibility: visible;
    }
  }
  &:not(.template-index) {
    .nov-footer {
      margin-top: 151px;
      @media (max-width: 991px) {
        margin-top: 70px;
      }
      @media (max-width: 767px) {
        margin-top: 53px;
      }
    }
  }
  .otCookiesNotification #cookies-wrapper {
    font-family: $font-family-default;
  }
}
.block_link_cart a {
  min-width: 230px;
  background-color: white;
  color: #222 !important;
  border: 2px solid black;
  @include transition(all 0.3s);
  text-align: center;
  font-weight: 700;
  &:hover {
    background-color: black;
    color: white !important;
  }
}
.cart_dropdown {
  #cart_block {
    position: relative;
    cursor: pointer;
