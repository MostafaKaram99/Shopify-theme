      @include border-radius(0);
      p.cart {
        padding: 20px;
      }
      .cart {
        background: $color-white;
        min-width: 315px;
        color: $color-heading;
        font-weight: 500;
        font-size: 13px;
        @include border-radius(0 0 4px 4px);
        @include box-shadow(0 0 15px 0 rgba(0, 0, 0, 0.1));
        &.cart_empty {
          padding: 30px;
          .empty_title {
            margin-top: 0 !important;
          }
        }
        .ajaxcart__inner {
          .ajaxcart__product {
            position: relative;
            padding: 20px;
            border-bottom: 1px solid #e6e6e6;
            .media {
              > a {
                img {
                  width: 70px;
                  @include rtl-margin-right(10px);
                  border: 1px solid #e6e6e6;
                }
              }
              .price_close {
                margin-top: 11px;
                @include flexbox();
                @include align-items(center);
                @include justify-content(space-between);
              }
            }
          }
          .product-name, .ajaxcart__product-meta {
            span {
              text-transform: none;
              font-size: 11px;
              color: $color-heading;
              font-weight: 600;
              line-height: 24px;
              display: block;
              &.title {
                font-weight: bold;
                margin-top: -5px;
                &:hover {
                  text-decoration-line: underline;
                  text-decoration-color: #000;
                }
              }
              &.bt_s {
                color: #909090;
              }
            }
          }
          .product-price {
            font-size: 10px;
            color: $color-heading;
            font-weight: 700;
            line-height: 22px;
          }
          .quantity {
            font-size: 1.2rem;
            color: $color-heading;
            font-weight: 500;
          }
        }
        .ajaxcart__footer {
          padding: 20px;
          .cart-note label {
            margin-top: 10px;
          }
          .subtotal {
            padding: 7px 0;
            margin-bottom: 10px;
            label, span {
              font-size: 1.4rem;
              color: $color-heading;
              font-weight: 900;
              text-transform: uppercase;
              margin: 0;
            }
          }
          .cart_view {
            display: block;
            margin: 0 60px;
            padding: 35px 0 10px;
          }
          .btn {
            height: 44px;
            line-height: 44px;
            width: 100%;
            padding: 0;
            background: #dcdcdc;
            @include border-radius(0);
            position: relative;
            overflow: hidden;
            span {
              font-size: 10px;
              font-weight: bold;
              color: $color-heading;
              z-index: 10;
              position: relative;
            }
            &:hover {
              background: #000;
              @include transition(all 0.5s ease);
              span {
                color: $color-white;
              }
            }
            &.cart__checkout {
              background: #000;
              margin-bottom: 10px;
              span {
                color: $color-white;
              }
            }
          }
        }
        .remove-from-cart {
          position: absolute;
          font-size: 1.6rem;
          color: #8f8f8f;
          display: inline-block;
          @include square(23px);
          text-align: center;
          @include border-radius(100%);
          top: 50%;
          transform: translateY(-50%);
          @include rtl-right(20px);
          background: #e6e6e6;
          &:hover {
            color: $theme-color-hover;
          }
        }
      }
