/* Site Header ================================================================================================================*/
.site-header {
  position: relative;
  &.open {
    position: fixed;
    width: 100%;
    top: 0;
    @include rtl-left(0);
    z-index: 99999;
  }
  @media (max-width: 991px) and (min-width: 768px) {
    .vertical_dropdown i {font-size: 21px;}
  }
  .header-center {
    padding: 40px 40px;
  }
  .label-header {
    font-size: 14px;
    color: {{settings.color_menu}};
    font-weight: 500;
  }
  .contentsticky_item {
    font-weight: 500;
    color: $theme-color-default;
    font-size: 11px;
    line-height: 22px;
  }
  .contentsticky_menu {position: initial;}
  .dropdown_currency {
    .dropdown-toggle {
      cursor: pointer;
      font-size: 10px;
      font-weight: 500;
      @include border-radius(3px);
      border: 1px solid rgba($color-white, 0.15);
      text-transform: uppercase;
      width: 50px;
      height: 26px;
      line-height: 23px;
      text-align: center;
      display: inline-block;
      &:after {
        display: none;
      }
    }
  }
  .header_link_myaccount, .link_wishlist {
    @include rtl-margin-right(15px);
    @include rtl-padding-right(15px);
    cursor: pointer;
    position: relative;
    &:after {
      content: "";
      position: absolute;
      width: 1px;
      height: 7px;
      background-color: #d4d4d4;
      top: 50%;
      @include translateY(-50%);
      @include rtl-right(0);
    }
  }
  .message_text {
    font-weight: 700;
    font-size: 1.2rem;
    text-transform: uppercase;
  }
  .wishlist {
    font-size: 1.4rem;
    color: #101010;
    &:hover {
      color: $theme-color-primary;
    }
  }
  @media (max-width: 991px) {
    .header-center {padding: 40px 15px;}
  }
}
.site-header__cart {
  position: relative;
  display: inline-block;
  cursor: pointer;
  .site-header__cart-icon {
    width: 23px;
    height: 23px;
    display: inline-block;
    background: url({{ 'icon_cart.png' | asset_url }}) no-repeat;
  }
  @media (max-width: 991px) {
    @include flexbox();
  }
}
.site-header__cart-count {
  display: inline-block;
  @include square(20px);
  line-height: 20px;
  text-align: center;
  background: {{settings.color_menu}};
  @include border-radius(100%);
  @include rtl-margin-left(7px);
  #CartCount {
    font-size: 10px;
    color: {{settings.background_color}};
    font-weight: bold;
    line-height: 22px;
  }
  @media (max-width: 991px) {
    position: absolute;
    top: -23px;
    @include rtl-left(-5px);
  }
}
#cart_block {
  position: relative;
  cursor: pointer;
  @include rtl-margin-left(37px);
  #cart-info {
    @include display(none);
    position: absolute;
    @include rtl-right(0);
    top: 100%;
    z-index: 99;
    padding-top: 40px;
    cursor: initial;
    @include border-radius(0);
    p.cart {
      padding: 20px;
    }
    .cart {
      background: $color-white;
      min-width: 315px;
      color: $color-heading;
      font-weight: 500;
      font-size: 13px;
      @include border-radius(0 0 4px 4px);
      @include box-shadow(0 0 15px 0 rgba(0, 0, 0, 0.1));
      .ajaxcart__inner {
        .ajaxcart__product {
          position: relative;
          padding: 20px;
          border-bottom: 1px solid #e6e6e6;
          .media {
            > a {
              img {
                width: 70px;
                @include rtl-margin-right(10px);
                border: 1px solid #e6e6e6;
              }
            }
            .price_close {
              margin-top: 11px;
              @include flexbox();
              @include align-items(center);
              @include justify-content(space-between);
            }
          }
        }
        .product-name, .ajaxcart__product-meta {
          span {
            text-transform: none;
            font-size: 11px;
            color: $color-heading;
            font-weight: 600;
            line-height: 24px;
            display: block;
            &.title {
              font-weight: bold;
              margin-top: -5px;
              &:hover {
                text-decoration-line: underline;
                text-decoration-color: #000;
              }
            }
            &.bt_s {
              color: #909090;
            }
          }
        }
        .product-price {
          font-size: 10px;
          color: $color-heading;
          font-weight: 700;
          line-height: 22px;
        }
        .quantity {
          font-size: 1.2rem;
          color: $color-heading;
          font-weight: 500;
        }
      }
      .ajaxcart__footer {
        padding: 20px;
        .cart-note label {
          margin-top: 10px;
        }
        .subtotal {
          padding: 7px 0;
          margin-bottom: 10px;
          label, span {
            font-size: 1.4rem;
            color: $color-heading;
            font-weight: 900;
            text-transform: uppercase;
            margin: 0;
          }
        }
        .cart_view {
          display: block;
          margin: 0 60px;
          padding: 35px 0 10px;
        }
        .btn {
          height: 44px;
          line-height: 44px;
          width: 100%;
          padding: 0;
          background: #dcdcdc;
          @include border-radius(0);
          position: relative;
          overflow: hidden;
          span {
            font-size: 10px;
            font-weight: bold;
            color: $color-heading;
            z-index: 10;
            position: relative;
          }
          &:hover {
            background: #000;
            @include transition(all 0.5s ease);
            span {
              color: $color-white;
            }
          }
          &.cart__checkout {
            background: #000;
            margin-bottom: 10px;
            span {
              color: $color-white;
            }
          }
        }
      }
      .remove-from-cart {
        position: absolute;
        font-size: 1.6rem;
        color: #8f8f8f;
        display: inline-block;
        @include square(23px);
        text-align: center;
        @include border-radius(100%);
        top: 50%;
        transform: translateY(-50%);
        @include rtl-right(20px);
        background: #e6e6e6;
        &:hover {
          color: $theme-color-hover;
        }
      }
    }
    .ajaxcart__inner {
      max-height: 333px;
      overflow-y: auto;
      margin-bottom: 27px;
      &::-webkit-scrollbar-track{
        background-color: #eaeaea;
        @include border-radius(20px);
      }
      &::-webkit-scrollbar {
        width: 5px;
        background-color: #eaeaea;
        @include border-radius(20px);
      }
      &::-webkit-scrollbar-thumb {
        @include border-radius(20px);
        background-color: $theme-color-primary;
      }
    }
    .threshold_spend {
      margin-bottom: 20px;
    }
    #threshold_bar_popup_minicart {
      padding: 0 20px;
    }
    .content_threshold.threshold_congrats {
      margin-top: 4px;
    }
  }
  .btn-success:not(:disabled):not(.disabled) {
    &.active:focus, &:active:focus, .show>.btn-success.dropdown-toggle:focus, .active, &:active, .show>.btn-success.dropdown-toggle {
      @include box-shadow(none);
      border-color: initial;
    }
  }
}
