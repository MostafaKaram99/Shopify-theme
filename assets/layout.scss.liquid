/* Breadcrumbs =============================================================================================== */
#NovBreadcrumbs {
  padding: 0 15px;
  position: relative;
  &:before {
    @include rtl-left(0);
  }
  .headingPage {
    font-weight: 800;
    font-size: 2.4rem;
    line-height: 40px;
    text-transform: uppercase;
    margin: 0 0 8px;
    position: relative;
    z-index: 1;
    text-align: center;
  }
  .breadcrumb {
    background: transparent;
    margin-bottom: 0;
    padding: 0;
    position: relative;
    z-index: 1;
    .list-inline-item {
      &:not(:last-child) {
        @include rtl-margin-right(10px);
        @include rtl-padding-right(10px);
        position: relative;
        &:before {
          content: "";
          @include square(4px);
          background: #fff;
          position: absolute;
          @include rtl-right(-2px);
          top: 50%;
          -webkit-transform: rotate(45deg) translateY(-50%);
          -ms-transform: rotate(45deg) translateY(-50%);
          transform: rotate(45deg) translateY(-50%);
        }
      }
      span {
        font-weight: 400;
        line-height: 28px;
      }
    }
  }
  .moved_product, .moved_blog {
    position: relative;
    z-index: 9;
    margin-top: 65px;
    a {
      @include flexbox();
      @include align-items(center);
      position: relative;
      &:not(:last-child) {
        @include rtl-margin-right(19px);
        @include rtl-padding-right(19px);
        &:after {
          content: '';
          display: block;
          width: 1px;
          height: 15px;
          background: rgba(255,255,255,0.2);
          position: absolute;
          top: 50%;
          @include rtl-right(0);
          @include translateY(-50%);
          
        }
      }
      i {
        font-size: 3rem;
        color: #fff;
        line-height: 20px;
        @include translateX(0);
        @include transition(all 0.5s ease);
        &:hover {
          &.zmdi-long-arrow-left {
            @include translateX(-10px);
            @include transition(all 0.5s ease);
          }
          &.zmdi-long-arrow-right {
            @include translateX(10px);
            @include transition(all 0.5s ease);
          }
        }
      }
    }
  }
  @media (max-width: 1199px) {
    padding: 95px 0 195px !important;
  }
  @media (max-width: 991px) {
    padding: 55px 0 100px !important;
  }
  @media (max-width: 767px) {
    padding: 55px 0 65px !important;
  }
}



